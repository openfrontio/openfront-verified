{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "npm run build-prod",
  "outputDirectory": "static",
  "framework": null,
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://api.eigenarcade.com/api/:path*"
    },
    {
      "source": "/w0/:path*",
      "destination": "https://api.eigenarcade.com/w0/:path*"
    },
    {
      "source": "/w1/:path*",
      "destination": "https://api.eigenarcade.com/w1/:path*"
    },
    {
      "source": "/:path((?!_next|api|static|favicon.ico|robots.txt|.*\\..*).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/(.*)\\.(js|css|svg|png|jpg|jpeg|webp|woff|woff2)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)\\.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "regions": ["sfo1"]
}
